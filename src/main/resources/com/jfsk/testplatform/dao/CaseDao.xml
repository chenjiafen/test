<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfsk.testplatform.dao.CaseDao">

    <select id="findAll" resultType="com.jfsk.testplatform.entity.Cases">
      select * from cases
    </select>
    <select id="findById" resultType="com.jfsk.testplatform.entity.Cases">
      select * from cases where id=#{id}
    </select>

</mapper>